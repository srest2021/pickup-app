{
  "expo": {
    "name": "Pickup!",
    "slug": "Pickup!",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/pickup-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/pickup-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#E6E6EA"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.expo.client"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#E6E6EA"
      },
      "package": "io.expo.client"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": ["expo-font"]
  }
}
